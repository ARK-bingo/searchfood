<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/personbackground2"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.appcompat.widget.AppCompatImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:fitsSystemWindows="true"
                android:scaleType="fitXY" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:orientation="vertical">

                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="57dp"
                    android:src="@drawable/p1" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_gravity="center"
                    android:layout_marginTop="0dp"
                    android:textColor="#ff333333"
                    android:textSize="23sp"
                    android:text="bb" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tv_id"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="账号: 123"
                    android:layout_gravity="center"
                    android:textColor="#ff333333"
                    android:textSize="16sp" />
            </LinearLayout>




        </RelativeLayout>
    </LinearLayout>



    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="400dp"
        android:layout_gravity="center"
        android:background="@drawable/try2">

        <RelativeLayout

            android:layout_width="300dp"
            android:layout_height="500dp">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_history"
                android:layout_width="33dp"
                android:layout_height="37dp"
                android:layout_alignBottom="@+id/tv_history"
                android:layout_marginStart="28dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="-5dp"
                android:src="@drawable/history" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_collect"
                android:layout_width="33dp"
                android:layout_height="37dp"
                android:layout_alignStart="@+id/iv_history"
                android:layout_alignBottom="@+id/tv_collect"
                android:layout_marginBottom="-3dp"
                android:src="@drawable/star3" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_photo"
                android:layout_width="33dp"
                android:layout_height="35dp"
                android:layout_alignStart="@+id/iv_collect"
                android:layout_alignBottom="@+id/tv_photo"
                android:layout_marginBottom="-3dp"
                android:src="@drawable/friend2" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_movie"
                android:layout_width="33dp"
                android:layout_height="37dp"
                android:layout_alignStart="@+id/iv_photo"
                android:layout_alignBottom="@+id/tv_movie"
                android:layout_marginBottom="-5dp"
                android:src="@drawable/person" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_location"
                android:layout_width="37dp"
                android:layout_height="38dp"
                android:layout_alignStart="@+id/iv_movie"
                android:layout_alignBottom="@+id/tv_location"
                android:layout_marginLeft="-3dp"
                android:layout_marginBottom="-5dp"
                android:src="@drawable/location" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_feedback"
                android:layout_width="33dp"
                android:layout_height="35dp"
                android:layout_alignStart="@+id/iv_movie"
                android:layout_alignBottom="@+id/tv_feedback"
                android:layout_marginStart="0dp"
                android:layout_marginBottom="-5dp"
                android:src="@drawable/feedback" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_set"
                android:layout_width="33dp"
                android:layout_height="35dp"
                android:layout_alignStart="@+id/iv_feedback"
                android:layout_alignBottom="@+id/tv_set"
                android:layout_marginStart="0dp"
                android:layout_marginBottom="-3dp"
                android:src="@drawable/set2" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_backhistory"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_alignBottom="@+id/tv_history"
                android:layout_alignParentRight="true"
                android:layout_marginRight="24dp"
                android:layout_marginBottom="10dp"
                android:src="@drawable/right" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_backphoto"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_alignStart="@+id/iv_backhistory"
                android:layout_alignBottom="@+id/tv_photo"
                android:layout_marginBottom="10dp"
                android:src="@drawable/right" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_backcollect"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_alignStart="@+id/iv_backphoto"
                android:layout_alignBottom="@+id/tv_collect"
                android:layout_marginBottom="10dp"
                android:src="@drawable/right" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_backmovie"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_alignStart="@+id/iv_backcollect"
                android:layout_alignBottom="@+id/tv_movie"
                android:layout_marginBottom="10dp"
                android:src="@drawable/right" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_backlocation"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_alignStart="@+id/iv_backmovie"
                android:layout_alignBottom="@+id/tv_location"
                android:layout_marginBottom="10dp"
                android:src="@drawable/right" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_backfeedback"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_alignStart="@+id/iv_backlocation"
                android:layout_alignBottom="@+id/tv_feedback"
                android:layout_marginBottom="10dp"
                android:src="@drawable/right" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/iv_backset"
                android:layout_width="12dp"
                android:layout_height="12dp"
                android:layout_alignStart="@+id/iv_backfeedback"
                android:layout_alignBottom="@+id/tv_set"
                android:layout_marginBottom="10dp"
                android:src="@drawable/right" />


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_history"
                android:layout_width="105dp"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginStart="60dp"
                android:layout_marginTop="22dp"
                android:text="  历史查询"
                android:textColor="#ff666666"
                android:textSize="22sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_collect"
                android:layout_width="95dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_history"
                android:layout_alignStart="@+id/tv_history"
                android:layout_marginTop="22dp"
                android:text="  收藏"
                android:textColor="#ff666666"
                android:textSize="22sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_photo"
                android:layout_width="95dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_collect"
                android:layout_alignStart="@+id/tv_collect"
                android:layout_marginTop="22dp"
                android:text="  动态"
                android:textColor="#ff666666"
                android:textSize="22sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_movie"
                android:layout_width="105dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_photo"
                android:layout_alignStart="@+id/tv_photo"
                android:layout_marginTop="22dp"
                android:text="  个人信息"
                android:textColor="#ff666666"
                android:textSize="22sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_location"
                android:layout_width="105dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_movie"
                android:layout_alignStart="@+id/tv_movie"
                android:layout_marginTop="22dp"
                android:text="  地址管理"
                android:textColor="#ff666666"
                android:textSize="22sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_feedback"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_location"
                android:layout_alignStart="@+id/tv_movie"
                android:layout_marginTop="22dp"
                android:text="  反馈与建议"
                android:textColor="#ff666666"
                android:textSize="22sp" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tv_set"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_feedback"
                android:layout_alignStart="@+id/tv_feedback"
                android:layout_marginTop="22dp"
                android:text="  设置"
                android:textColor="#ff666666"
                android:textSize="22sp" />


        </RelativeLayout>
    </LinearLayout>


</LinearLayout>
